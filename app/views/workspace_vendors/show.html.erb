<div class="container main">


    <div class="row">
      <div class="col">
        <h1><%= @workspaceVendor.name %></h1>
      </div>
    </div>

    <div class="row">
      <div class="col vendorTable">
        <table class="table">
          <thead>
            <tr>
              <th scope="col">Name</th>
              <th scope="col">Description</th>

            </tr>
          </thead>
          <tbody>
              <tr>
                <td><%= @workspaceVendor.name %></td>
                <td><%= @workspaceVendor.description %></td>
              </tr>
          </tbody>
        </table>
      </div>
    </div>



    <div class="row marginTop100">
      <div class="col">
        <h3>Transactions</h3>
      </div>
    </div>

    <div class="row">
      <div class="col">
        <%= render "workspace_vendors/vendorTransactions" %>
      </div>
    </div>

  
</div>

