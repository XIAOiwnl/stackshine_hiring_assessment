<div class="container main">
  <div class="row">
    <div class="col">
      <h1>Vendors</h1>
    </div>
  </div>

  <div class="row">
    <div class="col vendorsTable">
      <table class="table">
        <thead>
          <tr>
            <th scope="col">Name</th>
            <th scope="col">Description</th>
          </tr>
        </thead>
        <tbody>
          <% @workspaceVendors.each do |workspaceVendor| %>
            <tr>
              <td><%= link_to workspaceVendor.name, workspace_vendor_path(workspaceVendor.id) %></td>
              <td><%= workspaceVendor.description %></td>
            </tr>
          <% end %>

        </tbody>
      </table>
    </div>
  </div>
</div>
